<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiGi.UI.WPF.Core.Windows"
        xmlns:Controls="clr-namespace:DiGi.UI.WPF.Core.Controls" x:Class="DiGi.UI.WPF.Core.Windows.ListBoxWindow"
        mc:Ignorable="d"
        Title="ListBoxWindow" Height="450" Width="250">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <Controls:ListBoxControl x:Name="ListBoxControl_Main" HorizontalAlignment="Stretch" Margin="0,0,0,0" VerticalAlignment="Stretch"/>
        <Button x:Name="Button_Cancel" IsCancel="True" Content="Cancel" Grid.Row="1" Margin="0,0,9,0" Width="70" Height="21" HorizontalAlignment="Right" Grid.Column="1" Click="Button_Cancel_Click"/>
        <Button x:Name="Button_OK" Content="OK" Grid.Row="1" Margin="0,0,84,0" HorizontalAlignment="Right" Width="70" Height="21" Grid.Column="1" Click="Button_OK_Click"/>
    </Grid>
</Window>
